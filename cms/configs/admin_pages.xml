							<type>rent</type>
						</params>
           			</rent_add>
				</pages>
			</rent>
			<product_edit>
				<label>Edycja oferty</label>
				<module>cms</module>
				<controller>products</controller>
				<action>edit</action>
			</product_edit>
			<local>
				<label>Lokacje</label>
				<module>cms</module>
				<controller>local</controller>
				<action>index</action>
