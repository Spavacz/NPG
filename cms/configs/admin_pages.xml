				</pages>
			</articles>
			<sale>
				<label>Sprzedaż</label>
				<module>cms</module>
				<controller>products</controller>
				<action>index</action>
				<params>
					<type>sale</type>
				</params>
				<pages>
					<sale_add>
           				<label>Nowa oferta</label>
